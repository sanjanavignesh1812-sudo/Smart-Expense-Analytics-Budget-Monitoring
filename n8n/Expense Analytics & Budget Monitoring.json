      "id": "79cce912-b5dc-4eda-b916-976d499c0780",
      "name": "CATEGORY-WISE EXPENSE"
    },
    {
      "parameters": {
        "jsCode": "const result = {};\n\nfor (const item of items) {\n  const month = item.json.month;\n  const amount = Number(item.json.amount);\n\n  if (!result[month]) {\n    result[month] = 0;\n  }\n\n  result[month] += amount;\n}\n\nreturn Object.keys(result).map(m => ({\n  json: {\n    month: m,\n    total_amount: result[m]\n  }\n}));\n"
      },
