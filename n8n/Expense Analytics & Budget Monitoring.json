    {
      "parameters": {
        "jsCode": "const result = {};\n\nfor (const item of items) {\n  const category = item.json.category;\n  const amount = Number(item.json.amount);\n\n  if (!result[category]) {\n    result[category] = 0;\n  }\n\n  result[category] += amount;\n}\n\nreturn Object.keys(result).map(cat => ({\n  json: {\n    category: cat,\n    total_amount: result[cat]\n  }\n}));\n"
      },
      "type": "n8n-nodes-base.code",
